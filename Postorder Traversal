class Solution {
    ArrayList<Integer> postOrder(Node root) {
        ArrayList<Integer> result = new ArrayList<>();
        
        if (root.left != null) {
            result.addAll(postOrder(root.left));
        }
        if (root.right != null) {
            result.addAll(postOrder(root.right));
        }
        result.add(root.data);
        
        
        return result;
    }
}
